{
  "name": "@wdio/chrome-recorder",
  "version": "0.6.0",
  "description": "Generate WebdriverIO Tests from Google Chrome DevTools Recordings.",
  "main": "dist/index.js",
  "bin": "bin/wdio-chrome-recorder.js",
  "files": [
    "bin",
    "dist"
  ],
  "type": "module",
  "scripts": {
    "build": "tsc -p .",
    "clean": "rm -rf dist",
    "test": "run-s test:*",
    "test:lint": "eslint '**/*.{js,ts}' --fix .",
    "test:unit": "vitest --run",
    "watch": "tsc -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webdriverio/chrome-recorder.git"
  },
  "keywords": [
    "webdriverio",
    "wdio",
    "testing",
    "e2e",
    "automation",
    "devtools"
  ],
  "author": "Christian Bromann <mail@bromann.dev>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/webdriverio/chrome-recorder/issues"
  },
  "homepage": "https://github.com/webdriverio/chrome-recorder#readme",
  "devDependencies": {
    "@types/chai": "^4.3.14",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.12.7",
    "@types/prettier": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "chai": "^5.1.0",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "npm-run-all2": "^6.1.2",
    "release-it": "^17.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "vitest": "^1.5.0"
  },
  "dependencies": {
    "@puppeteer/replay": "^3.0.0",
    "chalk": "^5.3.0",
    "globby": "^14.0.1",
    "inquirer": "^10.0.0",
    "meow": "^13.2.0",
    "prettier": "^3.2.5"
  },
  "publishConfig": {
    "access": "public"
  }
}
