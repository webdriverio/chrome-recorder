{
  "name": "@wdio/chrome-recorder",
  "version": "0.5.2",
  "description": "Generate WebdriverIO Tests from Google Chrome DevTools Recordings.",
  "main": "dist/index.js",
  "bin": "bin/wdio-chrome-recorder.js",
  "files": [
    "bin",
    "dist"
  ],
  "type": "module",
  "scripts": {
    "build": "tsc -p .",
    "clean": "rm -rf dist",
    "test": "run-s test:*",
    "test:lint": "eslint '**/*.{js,ts}' --fix .",
    "test:unit": "vitest --run",
    "watch": "tsc -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webdriverio/chrome-recorder.git"
  },
  "keywords": [
    "webdriverio",
    "wdio",
    "testing",
    "e2e",
    "automation",
    "devtools"
  ],
  "author": "Christian Bromann <mail@bromann.dev>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/webdriverio/chrome-recorder/issues"
  },
  "homepage": "https://github.com/webdriverio/chrome-recorder#readme",
  "devDependencies": {
    "@types/chai": "^4.3.5",
    "@types/inquirer": "^9.0.3",
    "@types/node": "^20.2.3",
    "@types/prettier": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "@vitest/coverage-c8": "^0.33.0",
    "chai": "^5.1.0",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "npm-run-all": "^4.1.5",
    "release-it": "^17.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "vitest": "^0.31.1"
  },
  "dependencies": {
    "@puppeteer/replay": "^2.11.1",
    "chalk": "^5.2.0",
    "globby": "^14.0.1",
    "inquirer": "^9.2.6",
    "meow": "^12.0.1",
    "prettier": "^3.2.5"
  },
  "publishConfig": {
    "access": "public"
  }
}
